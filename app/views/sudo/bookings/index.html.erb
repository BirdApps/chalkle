<% set_titles %>
<% content_for :header do %>
<%= javascript_include_tag "sudo" %>
<% end %>

<%= render 'sub_menu' %>
<div class="even hidden-sm hidden-xs">
  <br /><br />
  <div class="row ">
    <div class="col-md-2">
      SwipeHQ TX
    </div>
    <div class="col-md-2">
     Chalkler
    </div>
    <div class="col-md-2">
      Date
    </div>
    <div class="col-md-2">
      Paid/Cost
    </div>
    <div class="col-md-2">
      Payment
    </div>
    <div class="col-md-2">
      Booking
    </div>
  </div>
</div>

<% if @bookings.empty? %>
  <div class="alert alert-warning center">
     No bookings found 
  </div>
<% end %>

<%= render paginate_these(@bookings) %>

<div class="row pagination-wrapper">
  <ul class="pagination">
    <li class="<%=  'disabled' if paginate_position == 0  %>">
      <a href="<%= url_for({page: paginate_position, take: params[:take], status: params[:status] }) %>">&laquo;</a>
    </li>
    <% paginate_count.times do |i| %>
      <li class="<%= 'active' if paginate_position == i %>">
        <a href="<%= url_for({page: i+1, take: params[:take], status: params[:status] }) %>">
          <%= i+1 %> 
          <% if paginate_position == i %>
            <span class="sr-only">(current)</span>
          <% end %>
        </a>
      </li>
    <% end %>
    <li class="<%= 'disabled' if paginate_position >= paginate_count-1 %>">
      <a href="<%= url_for({ page: paginate_position+2, take: params[:take], status: params[:status] }) %>">&raquo;</a>
    </li>
  </ul>
</div>

<%# high_chart 'signups', @bookings_chart %>
