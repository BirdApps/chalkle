.search_panel.full_page
  .container
    %ul.simple-nav#browse_type
      %li.active
        %a{:href => "#weeks", 'data-toggle' => "tab"}
          %i.fa.fa-list
          Week View
      %li
        %a{:href => "#months", 'data-toggle' => "tab"}
          %i.fa.fa-calendar
          Month View
    .tab-content
      #weeks.tab-pane.active
        = render partial: '/courses/weeks_calendar_view', locals: {week_courses: @week_courses}
      #months.tab-pane
        = render partial: '/courses/months_calendar_view', locals: {month_courses: @month_courses}
