- @month_lessons.each do |month, lessons|
  - if params[:format] == 'js'
    = render partial: 'month_calendar_view', locals: {month: month, lessons: lessons}
  - else
    .search_panel
      .container
        #months
          = render partial: 'month_calendar_view', locals: {month: month, lessons: lessons}


:coffeescript
  $("table.month_calendar_view").each ->
    $(this).monthlyCalendar()