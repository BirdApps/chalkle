        .search-filters
          .pull-right
            %form.form-search
              .input-append
                %input.span3.search-query{:type => "text"}/
                %button.btn{:type => "submit"}
                  %i.icon.icon-search
        %table.table.table-striped
          %thead
            %tr
              %th 
              %th ID
              %th Name
              %th Channel
              %th Money
              %th.delete-col
          %tbody
            -@data.each do |d|
              %tr
                %td
                  %a{href: "#"}
                    %i.icon.icon-pencil
                %td=d[:id]
                %td= link_to d[:name], "#"
                %td=d[:channel]
                %td=d[:money]
                %td
                  =link_to "#", confirm: "Really delete #{d[:name]}" do
                    %i.icon.color-red.icon-remove
            %tr.editing
              %td
                %a{href: "#"}
                  %i.icon.icon-arrow-left
              %td
              %td
                %input{type: "text", value: "Aldus Dumbledore"}
              %td
                %input{type: "text", value: "Gryffindor"}
              %td
                %input{type: "number", value: 100.50}
              %td
                =link_to "#" do
                  %i.icon.color-green.icon-ok
:javascript
  $(function(){
    //Show hide elements switch icon based off a button
    $('[data-toggle="showhide"]').on('click', function(){
      var href = $(this).attr('href')
      var $target = $($(this).attr('data-target') || (href && href.replace(/.*(?=#[^\s]+$)/, '')))
      var $icon = $(this).find('.icon')

      $target.toggleClass('hide');

      if($icon.length) {
        var inactive_icon_class = $(this).attr('data-inactive_icon_class')
        var active_icon_class = $(this).attr('data-active_icon_class');
        if($target.hasClass('hide')){
          $icon.removeClass(active_icon_class).addClass(inactive_icon_class);
        } else {
          $icon.removeClass(inactive_icon_class).addClass(active_icon_class);
        }
      }
    });

    //Make form row red on hover over destroy
    $('.destroy').on('mouseenter', function() {
      $(this).closest('tr').addClass('table-destroy');
    }).on('mouseleave', function(){
      $(this).closest('tr').removeClass('table-destroy');
    })

    $('#font-swap').on('click', function() { 
      $(this).closest('.tab-content').find('h1, h2, h3, h4').toggleClass('font-chalk');
    });
  });

