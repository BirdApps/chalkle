<li role="presentation" class="_notification <%= 'unactioned' unless notification.actioned? %>">
  <a href="<%= notification.href %>" /> 
    <div class="avatar64">
      <div class="img" style="background-image: url(<%= notification.image %>)">
      </div>
    </div>
    <p class="small margin-top-5">
      <%= quick_date_time notification.valid_from %>
    </p>
    <%= to_html(notification.message) %>
  </a>
</li>