<div class="teacher row">
  <div class="col-xs-8 col-sm-10">
    <img class="avatar32" src="<%= channel_admin.avatar %>">
    <div class="pull-left"> 
      <div class="name">
        <% if channel_admin.chalkler %>
          <%= link_to channel_admin.name, chalkler_path(channel_admin.chalkler.id) %>
        <% else %>
          <%= channel_admin.email %> <span class="small light">(user has not signed up)</span>
        <% end %>
      </div>
    </div>
  </div>
  <div class="col-xs-4 col-sm-2 text-right">
    <% if channel_admin.chalkler.blank? %>
      <%= link_to(edit_channel_admin_path(channel_admin.channel.url_name,channel_admin.id), class: 'btn btn-default') do %>
        <span class="fa fa-cog"></span> Edit 
      <% end %>
    <% end %>
  </div>
</div>